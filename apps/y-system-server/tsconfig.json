{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "../../dist/",  
      "rootDir": "./src",  
      "baseUrl": "./",
      "paths": {
        "@server/*": ["apps/y-system-server/src/*"],
      }
    },
    "include": [
      "src/**/*",
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  